        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Universe_Navigator"
        tools:targetApi="31">

        <meta-data
            android:name="com.yandex.maps.mobile.api_key"
            android:value="ced98ea9-1d35-4c75-ae0c-f46aaa5bcc29" />

        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".PlacesListActivity"
            android:exported="true">
        </activity>

        <activity
            android:name=".FirstBuildActivity"
            android:exported="true">
        </activity>

        <activity
            android:name=".SecondBuildActivity_18"
            android:exported="true">
        </activity>

        <activity
            android:name=".SecondBuildActivity_19"
            android:exported="true">
